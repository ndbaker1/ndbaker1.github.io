{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "verbatimModuleSyntax": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "types": [
      "vite/client"
    ],
    "noEmit": true,
    "isolatedModules": true
  },
  "include": [
    "src"
  ]
}
